<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidhello"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
       
        
        <activity android:name=".BMIActivity"
            android:label="BMI计算器"
            >  
            <intent-filter>
               	<action android:name="my.action"/>
               	<category android:name="android.intent.category.DEFAULT"/>            
            </intent-filter>        
        </activity>
        
         <activity
            android:name="com.example.androidhello.MainActivity"
            android:label="@string/app_name" >           
        </activity>
        
         <activity
            android:name="com.example.androidhello.SecondActivity"
            android:label="显示" >   
            <intent-filter>
               	<action android:name="my.action"/>
               	<category android:name="my.category"/>            
               	<category android:name="android.intent.category.DEFAULT"/>            
            </intent-filter>        
        </activity>
        
         <activity
            android:name="com.example.androidhello.FirstActivity"
            android:label="登录" >
        </activity>
        
         <activity
            android:name="com.example.androidhello.OneActivity"
            android:label="one" >
            
        </activity>
        
         <activity
            android:name="com.example.androidhello.TwoActivity"
            android:label="two" >
            
        </activity>
         <activity
            android:name="com.example.androidhello.MP3Activity"
            android:label="mp3" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
         <service android:name=".MyService" android:enabled="true"></service>
    </application>

</manifest>
